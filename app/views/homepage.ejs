<!DOCTYPE html>
<html lang="en">
<%- include('partials/head') %>
<body>
  <%- include('partials/header') %>
  <main class="homepage">
      <% for (const pokemon of pokemons) { %>
      <div class="card">
        <div class="card-img">
          <img src="img/<%= pokemon.numero %>.png" alt="Image de <%= pokemon.nom %>">
        </div>
        <div class="card-text">
          <h2>#<%= pokemon.numero %> <%= pokemon.nom %></h2>
        </div>
      </div>
      <% } %>
  </main>
</body>
</html>